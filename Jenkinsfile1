
pipeline {
    agent any
    tools{
        maven 'MAVEN_HOME'
    }
    stages {
        
        stage('Checkout') {
            steps {
             sh 'rm -rf port'
             sh 'git clone https://github.com/VINITHAS06/port.git'
            }
        }
        stage('deploy') {
            steps {
            configFileProvider([configFile(fileId: 'settings.xml_global', targetLocation: 'target')]) {
    // some block
}
            }
        }
         stage('deploy-maven') {
            steps {
            sh 'mvn deploy'
            }
            }
        }
        }
